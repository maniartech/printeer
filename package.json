{
  "name": "printeer",
  "version": "1.2.3",
  "description": "Prints specified web url to PDF (or Image - Coming Soon)!",
  "main": "index.js",
  "repository": "git@github.com:maniartech/printeer.git",
  "author": "Mohamed Aamir Maniar <aamir.maniar@maniartech.com>",
  "license": "ISC",
  "type": "module",
  "bin": {
    "printeer": "bin/index.js"
  },
  "scripts": {
    "build": "parcel build ./src/index.js",
    "build-cli": "parcel build ./src/cli.js & mv index.* ./bin",
    "parcel": "parcel",
    "esbuild": "esbuild",
    "esbuild-cli": "npm run esbuild -- ./src/cli.js --outfile=./bin/index.js --bundle --platform=node --packages=external --target=node16.8 --format=esm --sourcemap",
    "esbuild-lib": "npm run esbuild -- ./src/index.js --outfile=./index.js --bundle --platform=node --packages=external --target=node16.8 --format=esm --sourcemap"
  },
  "dependencies": {
    "puppeteer": "^19.4.1"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "constants-browserify": "^1.0.0",
    "esbuild": "^0.16.13",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "parcel": "^2.7.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "util": "^0.12.4"
  }
}
